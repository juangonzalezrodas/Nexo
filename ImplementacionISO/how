[1mdiff --git a/Nexo/vite-project/firestore.rules b/Nexo/vite-project/firestore.rules[m
[1mnew file mode 100644[m
[1mindex 0000000..fe45466[m
[1m--- /dev/null[m
[1m+++ b/Nexo/vite-project/firestore.rules[m
[36m@@ -0,0 +1,61 @@[m
[32m+[m[32mrules_version = '2';[m
[32m+[m[32mservice cloud.firestore {[m
[32m+[m[32m  match /databases/{database}/documents {[m
[32m+[m[41m    [m
[32m+[m[32m    // Funciones auxiliares[m
[32m+[m[32m    function isSignedIn() {[m
[32m+[m[32m      return request.auth != null;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    function isOwner(userId) {[m
[32m+[m[32m      return request.auth.uid == userId;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Reglas para usuarios[m
[32m+[m[32m    match /users/{userId} {[m
[32m+[m[32m      allow read: if isSignedIn();[m
[32m+[m[32m      allow create: if isSignedIn();[m
[32m+[m[32m      allow update, delete: if isOwner(userId);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Reglas para objetos[m
[32m+[m[32m    match /objects/{objectId} {[m
[32m+[m[32m      allow read: if isSignedIn();[m
[32m+[m[32m      allow create: if isSignedIn();[m
[32m+[m[32m      allow update: if isSignedIn();[m
[32m+[m[32m      allow delete: if false; // No permitir eliminaci√≥n[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Reglas para tickets[m
[32m+[m[32m    match /tickets/{ticketId} {[m
[32m+[m[32m      allow read: if isSignedIn();[m
[32m+[m[32m      allow create: if isSignedIn();[m
[32m+[m[32m      allow update: if isSignedIn() &&[m[41m [m
[32m+[m[32m                      (isOwner(resource.data.userId) ||[m[41m [m
[32m+[m[32m                       request.auth.token.role in ['admin', 'security']);[m
[32m+[m[32m      allow delete: if isOwner(resource.data.userId);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Reglas para coincidencias[m
[32m+[m[32m    match /matches/{matchId} {[m
[32m+[m[32m      allow read: if isSignedIn();[m
[32m+[m[32m      allow create: if isSignedIn();[m
[32m+[m[32m      allow update, delete: if request.auth.token.role in ['admin', 'security'];[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Reglas para notificaciones[m
[32m+[m[32m    match /notifications/{notificationId} {[m
[32m+[m[32m      allow read: if isSignedIn() && isOwner(resource.data.userId);[m
[32m+[m[32m      allow create: if isSignedIn();[m
[32m+[m[32m      allow update: if isOwner(resource.data.userId);[m
[32m+[m[32m      allow delete: if isOwner(resource.data.userId);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Reglas para entregas[m
[32m+[m[32m    match /deliveries/{deliveryId} {[m
[32m+[m[32m      allow read: if isSignedIn();[m
[32m+[m[32m      allow create: if isSignedIn();[m
[32m+[m[32m      allow update, delete: if request.auth.token.role in ['admin', 'security'];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
\ No newline at end of file[m
